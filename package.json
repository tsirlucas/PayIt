{
  "name": "PayIt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"react-native start\" ",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "rnlink": "react-native link",
    "lint": "tslint src/**/*.ts",
    "test": "jest --config ./jest.config.json",
    "appium-doctor": "appium-doctor",
    "e2e": "jest --testRegex=\"/e2e/.*.e2e.js\"",
    "e2e:android": "E2E_DEVICE=\"android-emulator\" yarn e2e",
    "e2e:ios": "E2E_DEVICE=\"ios-emulator\" yarn e2e",
    "tsc": "tsc",
    "clean": "rimraf lib",
    "build": "yarn clean && yarn tsc"
  },
  "e2e": {
    "devices": {
      "ios-emulator": {
        "platformName": "iOS",
        "platformVersion": "11.2",
        "deviceName": "iPhone 7",
        "app": "./ios/build/Build/Products/Debug-iphonesimulator/PayIt.app"
      },
      "android-emulator": {
        "platformName": "Android",
        "deviceName": "AndroidEmulator",
        "app": "./android/app/build/outputs/apk/app-x86-debug.apk"
      }
    }
  },
  "devDependencies": {
    "@types/es6-promise": "3.3.0",
    "@types/jest": "22.2.0",
    "@types/react": "16.0.38",
    "@types/react-native": "0.52.17",
    "@types/react-test-renderer": "16.0.1",
    "appium": "1.7.2",
    "appium-doctor": "1.4.3",
    "babel-core": "6.26.0",
    "babel-jest": "22.4.1",
    "babel-preset-react-native-stage-0": "1.0.1",
    "concurrently": "3.5.1",
    "jest": "22.4.2",
    "jest-react-native": "18.0.0",
    "react-test-renderer": "16.3.0-alpha.1",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.0",
    "tslint": "5.9.1",
    "typescript": "2.7.2",
    "wd": "^1.5.0"
  },
  "dependencies": {
    "native-base": "2.3.10",
    "react": "16.2.0",
    "react-native": "0.54.2",
    "react-native-firebase": "3.2.7",
    "react-native-router-flux": "4.0.0-beta.28"
  }
}
